# React env variables must be prefixed with REACT_APP_
# Copy this file to `.env` and fill in values for real Supabase mode.
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_ANON_KEY=

# Optional: URL used for email redirect after sign up (falls back to window.location.origin)
REACT_APP_FRONTEND_URL=http://localhost:3000

# Optional: Force MOCK mode even if Supabase vars are set ("true" | "false")
REACT_APP_MOCK_MODE=false
