# Create a copy of this file named ".env" in the same directory to configure the app.
# Note: For Create React App, only variables prefixed with REACT_APP_ are exposed to the browser.

# Supabase project URL, e.g. https://xxxxx.supabase.co
REACT_APP_SUPABASE_URL=

# Supabase anon public key
REACT_APP_SUPABASE_ANON_KEY=

# Optional: Frontend base URL for auth email redirects (defaults to window.location.origin)
REACT_APP_FRONTEND_URL=http://localhost:3000

# Toggle mock mode explicitly (true/false). If Supabase env vars are missing, mock mode is auto-enabled.
REACT_APP_MOCK_MODE=false
