# Web container environment example
# Copy to .env and set appropriate values.

# Existing variables (left unchanged if already in your environment)
REACT_APP_API_BASE=
REACT_APP_BACKEND_URL=
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false

# Supabase Auth (required to enable REAL auth mode)
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_ANON_KEY=

# Toggle mock mode (set to 'true' to disable Supabase and use local mock auth)
REACT_APP_MOCK_MODE=false
